<div class="ticket-data">
    <%if(tickets.length<=0){%>
        <div style="text-align: center;">There are no closed tickets</div>
        <%}else{%>
            <table class="ticket-table-container" cellspacing="0" cellpadding="0">
                <thead>
                    <tr class="ticket-table-header-container">
                        <th class="ticket-table-header-data">Ticket ID</th>
                        <th class="ticket-table-header-data">Subject</th>
                        <th class="ticket-table-header-data">Resolved By</th>
                        <th class="ticket-table-header-data">Status</th>
                        <th class="ticket-table-header-data">Created Date</th>
                        <th class="ticket-table-header-data">Resolved Date</th>
                    </tr>
                </thead>
                <%tickets.forEach(ticket=>{%>
                    <tr class="ticket-table-data-container">
                        <td class="ticket-table-data">
                            <%=ticket.ticketId%>
                        </td>
                        <td class="ticket-table-data">
                            <%=ticket.subject%>
                        </td>
                        <td class="ticket-table-data">
                            <%if(ticket.hasOwnProperty('assignedTo')){%>
                                <%= ticket.assignedTo.name %>
                                    <%}else{%>
                                        <p>himself</p>
                                        <%}%>
                        </td>
                        <td class="ticket-table-data">Closed</td>
                        <td class="ticket-table-data">
                            <time datetime="<%=ticket.createdAt%>"></time>
                        </td>
                        <td class="ticket-table-data">
                            <time datetime="<%=ticket.updatedAt%>"></time>
                        </td>
                    </tr>
                    <%})%>
            </table>
            <%}%>
</div>

<%- include('../includes/pagination.ejs',{url:'/admin/tickets/closed/',maxNumberStart:6,maxNumberMiddle:2}) %>