<% if(user && (user.userId!=post.owner.userId)){%>
    <div class="post-option">
        <div @click="ShowReportModal('post','<%=post._id%>','<%=post.postId%>')"> <svg class="post-option-icon"
                xmlns="http://www.w3.org/2000/svg" height="18px" viewBox="0 0 24 24" width="18px" fill="#FFFFFF">
                <path d="M0 0h24v24H0z" fill="none" />
                <path d="M14.4 6L14 4H5v17h2v-7h5.6l.4 2h7V6z" />
            </svg>
        </div>
        <div>
            <img loading="lazy" id="post-bookmark"
                class="<%= isBookmarked?'post-isBookmarked':'post-isNotBookmarked' %>  post-option-icon"
                @click="SetBookmark(<%= post.postId %>,<%= isBookmarked %>)">

        </div>

    </div>
    <%}%>