<div class="inventory-actions-container" v-if="GetItemPaginateData().totalPages >0">

    <div class="refresh-inventory-container hidden">
        <svg class="inventory-refresh-btn-icon" xmlns="http://www.w3.org/2000/svg" height="18px" viewBox="0 0 24 24"
            width="18px" fill="#FFFFFF">
            <path d="M0 0h24v24H0V0z" fill="none" />
            <path
                d="M17.65 6.35C16.2 4.9 14.21 4 12 4c-4.42 0-7.99 3.58-7.99 8s3.57 8 7.99 8c3.73 0 6.84-2.55 7.73-6h-2.08c-.82 2.33-3.04 4-5.65 4-3.31 0-6-2.69-6-6s2.69-6 6-6c1.66 0 3.14.69 4.22 1.78L13 11h7V4l-2.35 2.35z" />
        </svg>
        <div class="reload-btn-text">Reload Inventory</div>
    </div>
    <div class="item-paginate-container">
        <div @click="PaginateCreatePageItem(false)" class="item-paginate-btn">
            <svg class="item-paginate-icon" xmlns="http://www.w3.org/2000/svg" height="18px" viewBox="0 0 24 24"
                width="18px" fill="#FFFFFF">
                <path d="M0 0h24v24H0V0z" fill="none" />
                <path d="M15.61 7.41L14.2 6l-6 6 6 6 1.41-1.41L11.03 12l4.58-4.59z" />
            </svg>
        </div>
        <div class="item-paginate-text noSelect">
            {{ GetItemPaginateData().currentPage +' / '+GetItemPaginateData().totalPages }}
        </div>
        <div @click="PaginateCreatePageItem(true)" class="item-paginate-btn">
            <svg class="item-paginate-icon" xmlns="http://www.w3.org/2000/svg" height="18px" viewBox="0 0 24 24"
                width="18px" fill="#FFFFFF">
                <path d="M0 0h24v24H0V0z" fill="none" />
                <path d="M10.02 6L8.61 7.41 13.19 12l-4.58 4.59L10.02 18l6-6-6-6z" />
            </svg>
        </div>
    </div>
</div>